
@import "normalize.styl"
@import "nib"
@import "nib/vendor"


vendor-prefixes = o ms moz webkit official
azul = #1EB4DF
rojo = #FF241B
verde = #149152
amarillo = #FF8E1B
negro = #686868;
.azul
	color azul
.rojo
	color rojo
.amarillo
	color amarillo
.negro
	color negro


@keyframes animacionProyectos

	0%
		opacity:0
		transform:rotateX(90deg) translateZ(-25px) ;
	50%	
		opacity:1
		transform:rotateX(0deg) translateZ(-315px) ;
	100%
		opacity:0
		transform:rotateX(90deg) translateZ(-25px) ;


@keyframes poro1
	0%
		top -66% 
		left 10%
	25%
		top -20% 
		left 101%
	50%
		top -20% 
		left -1%
	75%
		top -55% 
		left 40%
	100%
		top -66% 
		left 10%
		
@keyframes poro2
	0%
		top -66% 
		left 101%
	25%
		top -60% 
		left 5%
	50%
		top -30% 
		left 50%
	75%
		top -25% 
		left 40%
	100%
		top -66% 
		left 101%

@keyframes poro3
	0%
		top -66% 
		left 50%
	25%
		top -30% 
		left -1%
	50%
		top -40% 
		left 101%
	75%
		top -25% 
		left 40%
	100%
		top -66% 
		left 50%
		
@keyframes poro4
	0%
		top -30% 
		left 50%
	25%
		top -66% 
		left 90%
	50%
		top -40% 
		left 60%
	75%
		top -30% 
		left 100%
	100%
		top -30% 
		left 50%

@keyframes amination
	0%
		transform rotate(0deg)
	100%
		transform rotate(360deg)
@keyframes amination2
	0%   
		transform rotate(0deg)
	100% 
		transform rotate(-360deg)
@keyframes amination3
	0%   
		transform rotate(0deg)
	100% 
		transform rotate(360deg)
@keyframes flechaAnimationP1
	0% 		
		transform rotate(0deg)
	20% 	
		transform rotate(10deg)
	40% 	
		transform rotate(0deg)
	60% 	
		transform rotate(-10deg)
	80% 	
		transform rotate(0deg)
	100% 	
		transform rotate(0deg)

@keyframes flechaAnimationP2
	from
		bottom 0
	to
		bottom 7px
		
.poritos
	width 10px
	height 10px
	top -66% 
	left 10%
	background-image url("../img/poritos.png")
	background-repeat: no-repeat;
	background-position: center center;
	background-size: cover;
	position relative
	opacity .5

.p1
	animation-name poro1
	animation-duration: 50s;
	animation-timing-function: linear;
	animation-iteration-count: infinite;
	
.p2
	animation-name poro2
	animation-duration: 50s;
	animation-timing-function: linear;
	animation-iteration-count: infinite;

.p3
	animation-name poro3
	animation-duration: 50s;
	animation-timing-function: linear;
	animation-iteration-count: infinite;
	
.p4
	animation-name poro4
	animation-duration: 50s;
	animation-timing-function: linear;
	animation-iteration-count: infinite;	

